{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "CosmosDb": {
    "DatabaseName": "MarketplaceDB",
    "ContainersNames": {
      "Users": "Users",
      "Products": "Products",
      "Carts": "Carts",
      "Orders": "Orders",
      "Categories": "Categories",
      "CarouselSlides": "CarouselSlides"
    }
  },
  "CacheSettings": {
    "EnableCaching": true,
    "CategoriesExpirationMinutes": 5,
    "ProductsExpirationMinutes": 5,
    "CarouselSlidesExpirationMinutes": 5,
    "SingleItemExpirationMinutes": 5
  }
}
